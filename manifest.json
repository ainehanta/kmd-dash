{
  "manifest_version": 2,
  "name": "KMD Dash",
  "version": "0.0.1",
  "icons": {
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": {
        "19": "logo.png",
        "38": "logo.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
  		"matches": [
        "http://www.kmd.keio.ac.jp/*",
        "http://archiver.kmd.keio.ac.jp/*"
      ],
  		"js": ["jquery-3.3.1.slim.min.js", "script.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
   },
   "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "http://www.kmd.keio.ac.jp/*",
    "http://archiver.kmd.keio.ac.jp/*"
  ]
}
